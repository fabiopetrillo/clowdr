{
    "name": "clowdr-actions",
    "version": "1.0.0",
    "description": "Action handlers",
    "main": "build/server.js",
    "scripts": {
        "build": "npm run build-shared && npm run gql-cdg && npm run build-this && npm run-script copy-resources",
        "build-this": "ttsc",
        "build-shared": "npm run --prefix=../../shared/ --cwd=../../shared/ build",
        "start": "node build/server.js",
        "dev": "npm run build-shared && nodenv -E ./.env -e \"npm run dev-stage2\"",
        "dev-stage2": "tsc-watch --compiler \"ttypescript/bin/tsc\" --onCompilationComplete \"npm run-script copy-resources\" --onSuccess \"npm run-script --prefix=../../ --cwd=../../ run-services-actions\"",
        "copy-resources": "rimraf build/resources && cpy --cwd=src/resources \"**/*\" ../../build/resources --parents",
        "test": "jest --config jest.config.ts --forceExit",
        "gql-cdg": "npx --no-install -c graphql-codegen -- --config codegen.js",
        "vimeo-client-cdg": "nodenv --exec ts-node \"scripts/get-vimeo-openapi.ts\" > src/generated/vimeo-openapi.json && oazapfts src/generated/vimeo-openapi.json src/lib/vimeoClient.ts"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/clowdr-app/clowdr.git"
    },
    "keywords": [
        "clowdr"
    ],
    "author": "Ed Nutting",
    "license": "BSD-3-Clause",
    "bugs": {
        "url": "https://github.com/clowdr-app/clowdr/issues"
    },
    "homepage": "https://github.com/clowdr-app/clowdr#readme",
    "dependencies": {
        "@aaronhayes/hasura-sdk": "^1.4.0",
        "@apollo/client": "^3.3.6",
        "@aws-sdk/client-cloudformation": "^3.1.0",
        "@aws-sdk/client-cloudfront": "^3.1.0",
        "@aws-sdk/client-elastic-transcoder": "^3.1.0",
        "@aws-sdk/client-iam": "^3.1.0",
        "@aws-sdk/client-mediaconvert": "^3.0.0",
        "@aws-sdk/client-medialive": "^3.1.0",
        "@aws-sdk/client-mediapackage": "^3.1.0",
        "@aws-sdk/client-s3": "^3.1.0",
        "@aws-sdk/client-sns": "^3.1.0",
        "@aws-sdk/client-transcribe": "^3.1.0",
        "@aws-sdk/credential-provider-env": "^3.1.0",
        "@aws-sdk/types": "^3.1.0",
        "@clowdr-app/shared-types": "file:../../shared",
        "@graphql-codegen/cli": "^1.19.4",
        "@graphql-codegen/fragment-matcher": "^2.0.1",
        "@graphql-codegen/introspection": "^1.18.1",
        "@graphql-codegen/typed-document-node": "^1.18.0",
        "@graphql-codegen/typescript": "^1.19.0",
        "@graphql-codegen/typescript-document-nodes": "^1.17.9",
        "@graphql-codegen/typescript-graphql-files-modules": "^1.18.1",
        "@graphql-codegen/typescript-graphql-request": "^2.0.2",
        "@graphql-codegen/typescript-operations": "^1.17.12",
        "@sendgrid/mail": "^7.4.0",
        "@types/cors": "^2.8.9",
        "@types/d3-format": "^2.0.0",
        "@types/express": "^4.17.9",
        "@types/html-to-text": "^6.0.0",
        "@types/node-fetch": "^2.5.7",
        "@types/ramda": "^0.27.34",
        "@types/sns-validator": "^0.3.0",
        "@types/uuid": "^8.3.0",
